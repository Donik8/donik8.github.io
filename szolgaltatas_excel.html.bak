        const nav = document.querySelector('nav');
        let prevScrollpas = window.pageYOffset;

        window.addEventListener('scroll',()=>{
            let currentScrollPas = window.pageYOffset;

            if(prevScrollpas<currentScrollPas){
                nav.classList.add('hide');
            }else{
                nav.classList.remove('hide');
            }

            prevScrollpas=currentScrollPas;
        })
